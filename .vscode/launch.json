  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug with Kotlin",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "program": "${workspaceFolder}/index.ts",
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "args": ["KOTLIN", "c:/Users/Alan/Documents/swagger.yaml"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug with Javscript",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "program": "${workspaceFolder}/index.ts",
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "args": ["JAVASCRIPT", "c:/Users/Alan/Documents/swagger.yaml"]
    }
  ]
}